var tween = null


func _ready():
	rect_pivot_offset = rect_size / 2
	tween = Tween.new()
	add_child(tween)


func play_animation():
	tween.interpolate_property(self, \"rect_scale\", Vector2(1.5, 1.5), 
			Vector2.ONE, 0.2, Tween.TRANS_SINE, Tween.EASE_OUT)
	tween.start()
"

